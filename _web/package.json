{
  "name": "_~vitesse-template~_",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "depend": "pnpm install --unsafe-perm --prefer-offline ${NPM_PROXY:+--registry ${NPM_PROXY}}",
    "build": "vite build",
    "set-version": "echo VITE_CHORE_VERSION=${IMAGE_TAG:-$(git describe --tags --first-parent --match 'v*' 2> /dev/null || echo v0.0.0)} > .env",
    "build-dev": "SOURCEMAP=true vite build",
    "serve": "vite preview",
    "build-front": "pnpm run set-version && pnpm run depend && pnpm build"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.37",
    "@tailwindcss/typography": "^0.5.2",
    "@tsconfig/svelte": "^2.0.1",
    "@types/codemirror": "^5.60.5",
    "@types/drawflow": "^0.0.4",
    "@types/path-browserify": "^1.0.0",
    "@types/showdown": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "codemirror": "^5.65.2",
    "drawflow": "^0.0.55",
    "eslint": "^8.3.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "highlight.js": "^11.4.0",
    "immutability-helper": "^3.1.1",
    "jwt-decode": "^3.1.2",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.4",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.5.0",
    "rollup": "^2.60.2",
    "sass": "^1.44.0",
    "showdown": "^2.0.0-alpha",
    "stylelint": "^14.1.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.0.0",
    "svelte": "3.46.4",
    "svelte-preprocess": "4.10.4",
    "svelte-spa-router": "^3.2.0",
    "tailwindcss": "^3.0.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "yaml": "^1.10.2"
  }
}
